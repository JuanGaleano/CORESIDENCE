##### DHS #####
# READ ALL DHS FILES ####
setwd("G:\\Shared drives\\CORESIDENCE\\WP2_DATA\\2_1_Input Data\\DHS_ORIGINALS")



patt.shp <- ".*RData*"

data.shp <-list.files(pattern=patt.shp)

sample_DHS_LIST <- sapply(data.shp, function(x) mget(load(x)), simplify = TRUE) 


